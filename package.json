{
  "name": "book-to-text",
  "version": "0.0.0",
  "description": "BookToText",
  "main": "app.js",
  "author": {
    "name": "Kristof Riebbels"
  },
  "scripts": {
    "build": "tsc --showConfig && tsc -p ./tsconfig.json",
    "clean": "rimraf ./dist",
    "start": "node ./dist/app.js",
    "debug": "node --inspect ./dist/app.js",
    "run": "npm run build && node ./dist/app.js"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.25",
    "@types/fs-extra": "^11.0.4",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^14.14.7",
    "@types/pdf-parse": "^1.1.4",
    "@types/turndown": "^5.0.5",
    "@types/yargs": "^17.0.32",
    "rimraf": "6.0.1",
    "typescript": "^4.5.2"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "dependencies": {
    "book-to-text": "file:",
    "canvas": "^3.0.0-rc2",
    "dotenv": "^16.4.5",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "markdown-it": "14.1.0",
    "openai": "4.53.2",
    "path": "0.12.7",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.8.69",
    "pdfml": "^2.0.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.4",
    "tesseract.js": "5.1.0",
    "tsyringe": "^4.8.0",
    "turndown": "^7.2.0",
    "turndown-plugin-gfm": "^1.0.2",
    "winston": "^3.13.0",
    "yargs": "^17.7.2"
  }
}
