{
  "name": "book-to-text",
  "version": "0.0.0",
  "description": "BookToText",
  "main": "app.js",
  "author": {
    "name": "Kristof Riebbels"
  },
  "scripts": {
    "build": "tsc --showConfig && tsc -p ./tsconfig.json",
    "clean": "rimraf ./dist",
    "start": "node ./dist/app.js",
    "debug": "node --inspect-brk ./dist/app.js",
    "run": "npm run build && node ./dist/app.js"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.25",
    "@types/fs-extra": "^11.0.4",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^14.14.7",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "dependencies": {
    "@types/sharp": "^0.32.0",
    "dotenv": "^16.4.5",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "11.2.0",
    "markdown-it": "14.1.0",
    "node-fetch": "3.3.2",
    "openai": "4.53.2",
    "path": "0.12.7",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.4",
    "tesseract.js": "5.1.0",
    "tsyringe": "^4.8.0",
    "winston": "^3.13.0",
    "yargs": "^17.7.2"
  }
}
